{
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "all_frames": true,
            "match_about_blank": true,
            "js": [
                "onthefly_config.js"
            ],
            "run_at": "document_start"
        },
        {
            "matches": [
                "<all_urls>"
            ],
            "all_frames": true,
            "match_about_blank": true,
            "js": [
                "ontheflyDOM-hidex.js"
            ],
            "run_at": "document_start"
        },
        {
            "all_frames": false,
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "run_at": "document_start",
            "css": [
                "content.css"
            ],
            "js": [
                "content.js"
            ]
        },
        {
            "all_frames": false,
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "run_at": "document_end",
            "js": [
                "content.end.js"
            ]
        }
    ],
    "update_url": "https://clients2.google.com/service/update2/crx",
    "description": "Fix position:fixed by adding transform:translateZ(0) to element. For smooth scrolling with 60fps.",
    "short_name": "fixfixed",
    "icons": {
        "128": "icon128.png",
        "48": "icon128.png",
        "16": "icon128.png"
    },
    "web_accessible_resources": [
        "background.css"
    ],
    "manifest_version": 2,
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "version": "1.4",
    "options_page": "options.html",
    "key": "",
    "permissions": [
        "storage",
        "tabs",
        "http://*/*",
        "https://*/*"
    ],
    "browser_action": {
        "name": "Click to toggle Fix-Fixed"
    },
    "name": "Smooth Scrolling with 60fps"
}