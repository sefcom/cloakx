{
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "all_frames": true,
            "match_about_blank": true,
            "js": [
                "onthefly_config.js"
            ],
            "run_at": "document_start"
        },
        {
            "matches": [
                "<all_urls>"
            ],
            "all_frames": true,
            "match_about_blank": true,
            "js": [
                "ontheflyDOM-hidex.js"
            ],
            "run_at": "document_start"
        },
        {
            "matches": [
                "<all_urls>"
            ],
            "all_frames": true,
            "js": [
                "highcontrast.js"
            ],
            "run_at": "document_start"
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "highcontrast-19.png",
        "default_title": "__MSG_HIGHCONTRAST_APPNAME__"
    },
    "name": "__MSG_HIGHCONTRAST_APPNAME__",
    "default_locale": "en",
    "icons": {
        "128": "highcontrast-128.png",
        "48": "highcontrast-48.png",
        "16": "highcontrast-16.png"
    },
    "description": "__MSG_HIGHCONTRAST_APPDESC__",
    "background": {
        "scripts": [
            "common.js",
            "background.js"
        ]
    },
    "version": "0.9.3",
    "manifest_version": 2,
    "update_url": "https://clients2.google.com/service/update2/crx",
    "permissions": [
        "<all_urls>",
        "tabs"
    ]
}