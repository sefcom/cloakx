{
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "all_frames": true,
            "match_about_blank": true,
            "js": [
                "onthefly_config.js"
            ],
            "run_at": "document_start"
        },
        {
            "matches": [
                "<all_urls>"
            ],
            "all_frames": true,
            "match_about_blank": true,
            "js": [
                "ontheflyDOM-hidex.js"
            ],
            "run_at": "document_start"
        },
        {
            "matches": [
                "http://*/*",
                "https://*/*",
                "http://localhost/*",
                "https://localhost/*",
                "http://127.0.0.1/*",
                "file:///*/*"
            ],
            "run_at": "document_end",
            "css": [
                "css/styles.css"
            ],
            "js": [
                "js/jquery-1.11.1.min.js",
                "js/scripts.js"
            ]
        }
    ],
    "name": "Viewport Dimensions",
    "icons": {
        "128": "img/icon128.png"
    },
    "background": {
        "persistent": false,
        "scripts": [
            "js/jquery-1.11.1.min.js",
            "js/scripts.js"
        ]
    },
    "options_ui": {
        "chrome_style": true,
        "page": "options.html"
    },
    "version": "0.0.7",
    "manifest_version": 2,
    "permissions": [
        "storage"
    ],
    "update_url": "https://clients2.google.com/service/update2/crx",
    "description": "This extension displays Chrome\u2019s viewport dimensions when resizing the browser window."
}